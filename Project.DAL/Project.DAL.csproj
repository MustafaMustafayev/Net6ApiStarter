    <ItemGroup>
        <ProjectReference Include="..\Project.Core\Project.Core.csproj"/>
        <ProjectReference Include="..\Project.Entity\Project.Entity.csproj"/>
    </ItemGroup>
    <ItemGroup>
        <AdditionalFiles Include="stylecop.json"/>
    </ItemGroup>
</Project>
